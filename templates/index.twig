{% set showSidebar = true %}

{% block pageLayout %}
  {% embed '_layouts/page/homePage' %}
    {% block contentLayout %}
      {% embed '_layouts/content/roharicky' %}

        {% block mainContent %}
          
            {# {% include "/_partials/carousel" %}
            {% include "/_partials/carouselJs" %} #}

          <div class="main-image">

            <!-- Image Map Generated by http://www.image-map.net/ -->
