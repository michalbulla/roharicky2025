                <area alt="Kontakt" title="Kontakt" href="/kontakt" coords="1715,616,1755,619,1814,624,1864,640,1880,661,1808,680,1723,675,1686,640" shape="poly">
            </map>

            <h1>{{ pageHeading }}</h1>
